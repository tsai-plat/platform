{
  "name": "tsai-admin",
  "version": "0.0.1",
  "description": "Tsailab admin application",
  "author": "lanbery",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "cross-env STAGE=dev nest start --debug --watch",
    "tsadmin": "cross-env STAGE=dev node dist/src/main",
    "prod": "node dist/main",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env STAGE=prod node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "8.1.0",
    "@tsai-platform/ucenter": "workspace:*",
    "@tsailab/common": "workspace:*",
    "@tsailab/ioredis-mq": "0.0.4",
    "@tsailab/node-redis": "workspace:*",
    "@tsailab/system": "workspace:*",
    "bcrypt": "5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.1",
    "cookie-parser": "1.4.7",
    "date-fns": "4.1.0",
    "express": "4.21.2",
    "helmet": "8.0.0",
    "iterare": "1.2.1",
    "mysql2": "3.11.5",
    "nanoid": "3.3.8",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "redis": "4.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "svg-captcha-fixed": "1.5.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@tsai-platform/core": "workspace:*",
    "@tsailab/schematics": "^1.2.6",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie-parser": "1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.0.0",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "add": "2.0.6",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@nestjs/typeorm": "^10.0.2",
    "@tsai-platform/core": "workspace:*",
    "chalk": "4.1.2",
    "typeorm": "^0.3.20"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}

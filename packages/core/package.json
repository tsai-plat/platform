{
    "name": "@tsai-platform/core",
    "version": "0.0.1",
    "description": "Tsailab library core",
    "author": "tsailab",
    "license": "LICENSE",
    "keywords": [
        "tsai-platform",
        "core"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm clean && pnpm build:pkg",
        "build:pkg": "tsc --build --verbose --force tsconfig.pkg.json",
        "clean": "rm -rf dist/*",
        "clean:lib": "tsc --clean tsconfig.lib.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "3.3.0",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/typeorm": "^10.0.2",
        "@tsailab/common": "workspace:*",
        "@types/multer": "1.4.12",
        "@types/request-ip": "0.0.41",
        "chalk": "4.1.2",
        "common": "0.2.5",
        "cos-nodejs-sdk-v5": "2.14.6",
        "js-yaml": "4.1.0",
        "multer": "1.4.5-lts.1",
        "request-ip": "3.3.0",
        "rxjs": "^7.8.1"
    },
    "peerDependencies": {
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "3.3.0",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/typeorm": "^10.0.2",
        "@types/request-ip": "0.0.41",
        "chalk": "4.1.2",
        "cos-nodejs-sdk-v5": "2.14.6",
        "js-yaml": "4.1.0",
        "multer": "1.4.5-lts.1",
        "request-ip": "3.3.0",
        "rxjs": "^7.8.1"
    },
    "peerDependenciesMeta": {},
    "dependencies": {
        "class-validator": "0.14.1",
        "redis": "4.7.0",
        "svg-captcha-fixed": "1.5.2"
    }
}
